{
    "name": "kyanag/revter",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Kyanag\\Revter\\": "src/",
        "files": [
            "functions.php"
        ]
    },
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "license": "MIT",
    "version": "2.0.0",
    "require": {
        "psr/http-message": "^1.0",
        "nikic/fast-route": "1.3",
        "symfony/event-dispatcher": "^4.4",
        "guzzlehttp/guzzle": "^6.5",
        "monolog/monolog": "^2.10",
        "php-di/php-di": "^6.3",
        "imangazaliev/didom": "^2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.11"
    },
    "scripts": {
        "run": "php main.php",
        "format": "phpcbf"
    }
}
